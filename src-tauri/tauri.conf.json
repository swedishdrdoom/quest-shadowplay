{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Quest Shadowplay",
  "identifier": "com.questshadowplay.app",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Quest Shadowplay",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  },
  "plugins": {}
}

